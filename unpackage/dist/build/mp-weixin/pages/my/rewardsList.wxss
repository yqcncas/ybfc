page{background:#f0f0f0}.noData{background:#fff;width:100%;height:100vh;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-orient:vertical;-webkit-box-direction:normal;-webkit-flex-direction:column;flex-direction:column;padding-top:100rpx}.rewardList{width:100%;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-orient:vertical;-webkit-box-direction:normal;-webkit-flex-direction:column;flex-direction:column;background:#fff}.reward-info{margin-left:30rpx;width:720rpx;height:110rpx;display:-webkit-box;display:-webkit-flex;display:flex;border-bottom:2rpx solid #f0f0f0}.reward-detail{-webkit-box-flex:1;-webkit-flex:1;flex:1;height:110rpx;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-orient:vertical;-webkit-box-direction:normal;-webkit-flex-direction:column;flex-direction:column}.reward-detail > view:first-child{margin-top:10rpx;font-size:24rpx;color:#828282}.reward-detail > view:nth-child(2){margin-top:10rpx;font-size:28rpx;color:#4a4a4a;white-space:nowrap;text-overflow:ellipsis;overflow:hidden}.reward-num{-webkit-box-flex:1;-webkit-flex:1;flex:1;height:110rpx;line-height:110rpx;text-align:right}.reward-num > .reward-tit{font-size:24rpx;color:#a6a6a6;margin-right:20rpx}.reward-num > .reward-add{font-size:36rpx;color:#55a900;padding-right:30rpx}.reward-num > .reward-reduce{font-size:36rpx;color:#d0021b;padding-right:30rpx}