page {
  background: #F0F0F0;
}
.followList {
  width: 750rpx;
  background: #FFFFFF;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.followList > view:first-child {
  width: 690rpx;
  margin: 30rpx 30rpx 0 30rpx;
  font-size: 28rpx;
  color: #333333;
}
.followList > view:last-child {
  width: 690rpx;
  margin: 20rpx 30rpx 30rpx 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.followList > view:last-child > view:first-child {
  font-size: 24rpx;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  color: #9B9B9B;
}
.followList > view:last-child > view:last-child {
  font-size: 24rpx;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  color: #9B9B9B;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
}
.followList + .followList {
  border-top: 1px solid #F5F5F5;
}
.noData {
  background: #FFFFFF;
  width: 100%;
  height: 100vh;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  padding-top: 100rpx;
}

