<view><view class="client"><block wx:for="{{clientList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['handleToCheckRecord',['$0'],[[['clientList','',index,'id']]]]]]]}}" class="client-list" bindtap="__e"><view class="client-avatar"><image src="{{item.avatar}}" mode></image></view><view class="client-info"><view class="client-detail"><view><text>{{item.name}}</text><view data-event-opts="{{[['tap',[['handleToEdit',['$0',index,'$1'],[[['clientList','',index,'id']],[['clientList','',index,'name']]]]]]]}}" catchtap="__e">修改</view></view><view><text>{{item.mobile}}</text></view></view><view class="{{['client-status',[(item.status==1)?'blue':'',(item.status==2)?'orange':'',(item.status==10)?'green':'',(item.status==11)?'red':'']]}}"><block wx:if="{{item.status==1}}"><text>审核中</text></block><block wx:if="{{item.status==2}}"><text>未注册</text></block><block wx:if="{{item.status==10}}"><text>已通过</text></block><block wx:if="{{item.status==11}}"><text>未通过</text></block></view></view></view></block></view><block wx:if="{{!isEmpty}}"><uni-load-more vue-id="1d13d5c2-1" status="{{status}}" contentText="{{contentText}}" bind:__l="__l"></uni-load-more></block><block wx:if="{{isEmpty}}"><view class="noData"><u-empty vue-id="1d13d5c2-2" text="暂无数据哦~" mode="favor" bind:__l="__l"></u-empty></view></block><edit-modal vue-id="1d13d5c2-3" show="{{modalShow}}" name="{{userName}}" data-event-opts="{{[['^close',[['modalClose']]],['^code',[['getCode']]]]}}" bind:close="__e" bind:code="__e" bind:__l="__l"></edit-modal></view>