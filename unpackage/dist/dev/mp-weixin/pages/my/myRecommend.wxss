page {
  background: #F0F0F0;
}
.header {
  width: 100%;
  height: 90rpx;
  position: fixed;
  z-index: 10;
  top: 0;
  border-bottom: 2rpx solid #F0F0F0;
  background: #FFFFFF;
}
.title {
  margin-left: 30rpx;
  width: 690rpx;
  height: 90rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.title > view {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 28rpx;
  color: #A6A6A6;
  text-align: center;
}
.client {
  width: 100%;
  background: #FFFFFF;
  margin-top: 90rpx;
}
.client-list {
  margin-left: 30rpx;
  width: 720rpx;
  height: 100rpx;
  border-bottom: 2rpx solid #F0F0F0;
}
.client-info {
  width: 690rpx;
  height: 100rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.client-info > view {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  height: 100rpx;
  font-size: 28rpx;
  color: #5B5B5B;
  text-align: center;
  line-height: 100rpx;
}
.client-info .green {
  color: #55A900;
}
.client-info .blue {
  color: #4A90E2;
}
.client-info .red {
  color: #D0021B;
}
.client-info .orange {
  color: #FE7838;
}
.noData {
  background: #FFFFFF;
  width: 100%;
  height: 100vh;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  padding-top: 200rpx;
}

