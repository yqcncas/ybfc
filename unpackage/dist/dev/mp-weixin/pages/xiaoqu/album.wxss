page {
  background: #F0F0F0;
}
.pic1 {
  width: 100%;
  background: #FFFFFF;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  padding: 30rpx 30rpx;
  margin-bottom: 30rpx;
}
.pic1 > view:first-child {
  width: 690rpx;
  font-size: 28rpx;
  color: #5B5B5B;
}
.pic1 > view:nth-child(2) {
  width: 690rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.pic1 > view:nth-child(2) > view {
  margin-left: 15rpx;
  width: 210rpx;
  height: 210rpx;
  border-radius: 4rpx;
  margin-top: 18rpx;
}
.pic1 > view:nth-child(2) > view > image {
  width: 210rpx;
  height: 210rpx;
  border-radius: 4rpx;
}

