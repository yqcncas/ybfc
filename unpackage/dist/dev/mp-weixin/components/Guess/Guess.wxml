<view><view class="guess"><block wx:if="{{houseType==1||houseType==2}}"><view class="guess-tit">猜你喜欢</view></block><block wx:if="{{houseType==3||houseType==4}}"><view class="guess-tit">看了该楼盘的人还看了</view></block><view class="homeList"><block wx:for="{{houseList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{houseType==1||houseType==2}}"><view data-event-opts="{{[['tap',[['checkDetail',['$0'],[[['houseList','',index,'houseId']]]]]]]}}" class="home-info" bindtap="__e"><view class="home-pic"><image src="{{item.picUrl[0]}}" lazy-load="true"></image></view><view class="home-detail"><view class="homeName"><text>{{item.houseName}}</text></view><view class="home-address"><text space="nbsp">{{item.provinceString+" |"+item.areaName}}</text><block wx:if="{{isAdmin}}"><text>{{"|"+item.buildNum+"栋"+item.roomNum}}</text></block></view><view class="home-tips"><block wx:for="{{item.label}}" wx:for-item="it" wx:for-index="labIndex" wx:key="labIndex"><view class="{{[[(labIndex==0)?'tips-1':'',(labIndex==1)?'tips-2':'',(labIndex==2)?'tips-3':'']]}}"><text>{{it}}</text></view></block></view><block wx:if="{{houseType==2}}"><view class="home-price"><text>{{item.price+"万("+item.acreage+"m²|"+item.unitPrice+"元/m²)"}}</text></view></block><block wx:if="{{houseType==1}}"><view class="home-price"><text>{{item.rental+"元/月("+item.acreage+"m²)"}}</text></view></block></view></view></block></block><block wx:for="{{houseList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{houseType==3||houseType==4}}"><view data-event-opts="{{[['tap',[['checkNewDetail',['$0','$1'],[[['houseList','',index,'areaId']],[['houseList','',index,'houseId']]]]]]]}}" class="home-info" bindtap="__e"><view class="home-pic"><image src="{{item.picUrl[0]}}" lazy-load="true" mode="scaleToFill"></image></view><view class="home-detail"><view class="homeName"><block wx:if="{{houseType==3}}"><text>{{item.areaName}}</text></block><block wx:if="{{houseType==4}}"><text>{{item.houseName}}</text></block></view><view class="home-address"><block wx:if="{{item.provinceString}}"><text space="nbsp">{{item.provinceString}}</text></block></view><view class="home-tips"><block wx:for="{{item.label}}" wx:for-item="it" wx:for-index="labIndex" wx:key="labIndex"><view class="{{[[(labIndex==0)?'tips4':'',(labIndex!==0)?'tips5':'']]}}"><text>{{it}}</text></view></block></view><view class="home-price"><block wx:if="{{houseType==3}}"><text>{{item.price+"元/m²(楼盘均价)"}}</text></block><block wx:if="{{houseType==4&&item.price}}"><text>{{item.price+"万"}}</text></block><block wx:if="{{houseType==4&&!item.price}}"><text>暂无售价</text></block></view></view></view></block></block></view></view></view>