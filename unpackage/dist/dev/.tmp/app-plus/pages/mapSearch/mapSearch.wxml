<view><view class="tab"><scroll-view class="nav-bar" id="nav-bar" scroll-x scroll-with-animation scroll-left="{{scrollLeft}}"><block wx:for="{{tabList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="{{['nav-item',(index===tabCurrentIndex)?'current':'']}}" id="{{'tab'+index}}" data-event-opts="{{[['tap',[['changeTab',[index]]]]]}}" bindtap="__e">{{item.name}}</view></block></scroll-view></view><view><map latitude="{{lat}}" longitude="{{lng}}" scale="{{scale}}" markers="{{markers}}" circles="{{circles}}" id="mapSearch" enable-rotate="true" data-event-opts="{{[['regionchange',[['changeRegion',['$event']]]],['labeltap',[['clickLabel',['$event']]]]]}}" bindregionchange="__e" bindlabeltap="__e"></map></view></view>