.followUp {
  width: 100%;
  background: #FFFFFF;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.followUpList {
  width: 720rpx;
  margin-left: 30rpx;
  border-bottom: 2rpx solid #F0F0F0;
}
.followUpInfo {
  width: 690rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.followUpInfo > view:first-child {
  margin-top: 18rpx;
  width: 690rpx;
  font-size: 28rpx;
  color: #4A4A4A;
  letter-spacing: 0.34rpx;
}
.followUpInfo > view:nth-child(2) {
  margin-top: 10rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 690rpx;
  font-size: 24rpx;
  color: #9B9B9B;
  letter-spacing: 0.28rpx;
  margin-bottom: 18rpx;
}
.followUpInfo > view:nth-child(2) > text:first-child {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.followUpInfo > view:nth-child(2) > text:nth-child(2) {
  text-align: right;
}

