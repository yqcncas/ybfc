.content {
  width: 590rpx;
  height: 1048rpx;
  margin: 30rpx 80rpx 0 80rpx;
  border-radius: 10rpx;
}
.bottom {
  width: 750rpx;
  height: 100rpx;
  position: fixed;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  bottom: 0;
}
.bottom > view:first-child {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  color: #FFFFFF;
  font-size: 28rpx;
  background-image: -webkit-linear-gradient(45deg, #FFBF2F 0%, #FF9417 100%);
  background-image: linear-gradient(45deg, #FFBF2F 0%, #FF9417 100%);
}
.bottom > view:first-child image {
  width: 34rpx;
  height: 34rpx;
}
.bottom > view:first-child text {
  margin-left: 20rpx;
}
.bottom > view:last-child {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  background-image: -webkit-linear-gradient(45deg, #AC63C0 0%, #874A98 100%);
  background-image: linear-gradient(45deg, #AC63C0 0%, #874A98 100%);
}
.bottom > view:last-child button {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  color: #FFFFFF;
  font-size: 28rpx;
  background-image: -webkit-linear-gradient(45deg, #AC63C0 0%, #874A98 100%);
  background-image: linear-gradient(45deg, #AC63C0 0%, #874A98 100%);
}
.bottom > view:last-child button image {
  width: 34rpx;
  height: 34rpx;
}
.bottom > view:last-child button text {
  margin-left: 20rpx;
}

